{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.5.2"
        }
    },
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/$1/$2"
        },
        {
            "src": "/robots.txt",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/robots.txt"
        },
        {
            "src": "/404.svg",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/404.svg"
        },
        {
            "src": "/link.svg",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/link.svg"
        },
        {
            "src": "/favicon.ico",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/sw.js",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/sw.js"
        },
        {
            "src": "/offline.html",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/offline.html"
        },
        {
            "src": "/manifest.webmanifest",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/manifest.webmanifest"
        },
        {
            "src": "/icon-192x192.png",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/icon-192x192.png"
        },
        {
            "src": "/icon-256x256.png",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/icon-256x256.png"
        },
        {
            "src": "/icon-384x384.png",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/icon-384x384.png"
        },
        {
            "src": "/icon-512x512.png",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/public/icon-512x512.png"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "dikitlink",
        "BASEURL": "https://dikitlink.vercel.app",
        "HTTPS": "true"
    }
}