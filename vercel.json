{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.5.2"
        }
    },
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/404.svg",
            "dest": "/404.svg"
        },
        {
            "src": "/link.svg",
            "dest": "/link.svg"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/sw.js",
            "dest": "/sw.js"
        },
        {
            "src": "/offline.html",
            "dest": "/offline.html"
        },
        {
            "src": "/manifest.webmanifest",
            "dest": "/manifest.webmanifest"
        },
        {
            "src": "/icon-192x192.png",
            "dest": "/icon-192x192.png"
        },
        {
            "src": "/icon-256x256.png",
            "dest": "/icon-256x256.png"
        },
        {
            "src": "/icon-384x384.png",
            "dest": "/icon-384x384.png"
        },
        {
            "src": "/icon-512x512.png",
            "dest": "/icon-512x512.png"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "dikitlink",
        "BASEURL": "https://dikitlink.vercel.app/",
        "HTTPS": "true"
    }
}