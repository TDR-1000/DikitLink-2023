{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.5.2"
        }
    },
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "/public/$1/$2",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/404.svg",
            "dest": "/public/404.svg",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/link.svg",
            "dest": "/public/link.svg",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/sw.js",
            "dest": "/public/sw.js",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/offline.html",
            "dest": "/public/offline.html",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/manifest.webmanifest",
            "dest": "/public/manifest.webmanifest",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/icon-192x192.png",
            "dest": "/public/icon-192x192.png",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/icon-256x256.png",
            "dest": "/public/icon-256x256.png",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/icon-384x384.png",
            "dest": "/public/icon-384x384.png",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/icon-512x512.png",
            "dest": "/public/icon-512x512.png",
            "headers": {
                "Cache-Control": "public, max-age=604800, must-revalidate"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "dikitlink",
        "BASEURL": "https://dikitlink.vercel.app/",
        "HTTPS": "true"
    }
}